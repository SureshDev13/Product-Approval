<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Number_is_numeric</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISNUMBER( AccountNumber ))</errorConditionFormula>
    <errorDisplayField>AccountNumber</errorDisplayField>
    <errorMessage>Enter only number</errorMessage>
</ValidationRule>
