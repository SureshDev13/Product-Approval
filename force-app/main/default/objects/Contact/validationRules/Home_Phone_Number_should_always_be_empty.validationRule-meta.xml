<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Home_Phone_Number_should_always_be_empty</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISBLANK(Phone),
  NOT(ISBLANK( HomePhone ))
)</errorConditionFormula>
    <errorDisplayField>HomePhone</errorDisplayField>
    <errorMessage>Phone Number is required and Home Phone Number should always be empty</errorMessage>
</ValidationRule>
